{
  "expo": {
    "name": "app01100",
    "slug": "app01100",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hedilarbi912.app01100"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.hedilarbi912.app01100"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permettre à $(PRODUCT_NAME) d'utiliser votre emplacement."
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "Permettre à $(PRODUCT_NAME) d'utiliser votre caméra."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "831aec09-8b4a-4b7e-ae65-646db34af8f3"
      }
    }
  }
}
